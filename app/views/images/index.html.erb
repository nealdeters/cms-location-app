<table class="table table-hover">
  <thead>
    <tr>
      <th>Image ID</th>
      <th>Image Name</th>
      <th>Updated at</th>
      <th>Created at</th>
      <th>View</th> 
      <th>Edit</th>
      <th>Delete</th>  
    </tr>
  </thead>
<% @images.each do |image| %>
  <tbody>
    <tr>
      <td><%= image.id %></td>
      <td><%= image.image_name %></td>
      <td><%= image.updated_at %></td>
      <td><%= image.created_at %></td>
      <td>
        <%= link_to "View", brand_content_show_path(:id => content.id), class: "btn btn-default" %>
      </td>
      <td>
        <%= link_to "Edit", brand_content_edit_path(:id => content.id), class: "btn btn-info" %>
      </td> 
      <td>
        <%= link_to "Delete", brand_content_delete_path(:id => content.id), method: :delete, class: "btn btn-danger", data: {confirm: "Are you sure?"}  %>
      </td>  
    </tr>
  </tbody>
<% end %>
</table>
<%= link_to "Add new content", brand_content_new_path(@brand), class: "btn btn-default" %>