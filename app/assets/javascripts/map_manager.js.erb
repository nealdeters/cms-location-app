var MapManager = function ()  {
  var map;
  var marker;

  this.initMap = function (obj) {
    map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: obj.lat , lng: obj.long},
      zoom: 14,
      scrollwheel: false
    });

    marker = new google.maps.Marker({
      position: {lat: obj.lat , lng: obj.long},
      map: map,
    });
  }
}
